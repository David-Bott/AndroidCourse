<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.administrator.androidcourse">
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Course.Lesson01" />
        <activity android:name=".Course.Lesson02" />
        <activity android:name=".Course.Lesson03" />
        <activity android:name=".Course.Lesson04" />
        <activity android:name=".Course.Lesson05" />
        <activity android:name=".Course.Lesson05_2" />
        <activity android:name=".Course.Lesson06" />
        <activity android:name=".Course.Lesson07" />
        <activity android:name=".Course.Lesson07_02"></activity>
    </application>

</manifest>